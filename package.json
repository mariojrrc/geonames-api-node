{
  "name": "geonames-api-node",
  "version": "1.0.9",
  "description": "Example of using Node.JS with mongodb",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prod": "NODE_PATH=. NODE_ENV=production node index.js",
    "dev": "NODE_PATH=. NODE_ENV=development nodemon index.js",
    "stage": "NODE_PATH=. NODE_ENV=staging node index.js",
    "release": "git pull && release-it --no-npm.publish",
    "patch": "git pull && release-it patch --no-npm.publish",
    "minor": "git pull && release-it minor --no-npm.publish",
    "major": "git pull && release-it major --no-npm.publish",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --unhandled-rejections=strict",
    "test-ci": "NODE_ENV=test jest --coverage=false --detectOpenHandles --forceExit --unhandled-rejections=strict",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": {
    "name": "Mario Costa",
    "email": "mariojr.rcosta@gmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "awilix": "^10.0.2",
    "awilix-koa": "^12.0.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "hal": "^1.2.0",
    "http-status": "^1.7.4",
    "is-uuid": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.8.2",
    "koa-body": "^6.0.1",
    "koa-compress": "^5.1.0",
    "koa-helmet": "^7.1.0",
    "koa-locales": "^1.12.0",
    "koa-logger": "^3.2.1",
    "koa-mongo-crud": "^1.2.5",
    "koa-qs": "^3.0.0",
    "koa-ratelimit": "^5.0.1",
    "koa-router": "^14.0.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-swagger-ui": "^5.4.0",
    "koa2-useragent": "^0.3.2",
    "lodash": "^4.17.21",
    "lodash.has": "^4.5.2",
    "lodash.partition": "^4.6.0",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "mongodb": "^6.11.0",
    "newrelic": "^11.4.0",
    "objects-to-csv": "^1.3.6",
    "query-string": "^9.1.1",
    "supertest": "^7.0.0",
    "uuid": "^11.0.3",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.25.2",
    "@babel/preset-env": "^7.25.2",
    "axios": "^1.7.7",
    "babel-jest": "^29.7.0",
    "commander": "^12.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-stare": "^2.4.0",
    "lint-staged": "^15.2.10",
    "lodash.defaultsdeep": "^4.6.1",
    "nodemon": "^3.1.7",
    "patch-package": "^8.0.1",
    "prettier": "^3.3.3",
    "release-it": "^17.9.0",
    "superagent-defaults": "^0.1.14",
    "supertest": "^7.0.0"
  },
  "overrides": {
    "koa-mongo-crud": {
      "mongodb": "^6.11.0"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
